.loading-spinner {
  display: inline-block;
  width: 1.5rem;
  height: 1.5rem;
  border-radius: 100%;
  border: 0.1875rem solid transparent;
  border-color: var(--snow-dark);
  border-left: 0.1875rem solid transparent;
  -webkit-animation: spin 1s linear infinite;
  animation: spin 1s linear infinite;
}
.loading-spinner--small {
  width: 0.75rem;
  height: 0.75rem;
}
.loading-spinner--white {
  border-color: #fff #fff #fff transparent;
  border-bottom-color: var(--white);
  border-right-color: var(--white);
  border-top-color: var(--white);
}
@-webkit-keyframes spin {
  to {
    transform: rotate(1turn);
  }
}
@keyframes spin {
  to {
    transform: rotate(1turn);
  }
}
#menu.open {
  display: block;
  position: fixed;
  z-index: 4;
  top: 0.5rem;
  top: var(--spacing-extra-small);
  right: 0.5rem;
  right: var(--spacing-extra-small);
  left: 0.5rem;
  left: var(--spacing-extra-small);
}
@media screen and (min-width: 62.5em) {
  #menu.open {
    display: none;
  }
}
.menu-current-product:before {
  content: " ";
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 0.25rem;
  width: var(--spacing-extra-extra-small);
  background-color: #a3aacc;
  background-color: var(--silver);
  border-top-right-radius: 2px;
  border-bottom-right-radius: 2px;
}
.mt-auto {
  margin-top: auto;
}
.mb-auto {
  margin-bottom: auto;
}
.minh-vh-100 {
  min-height: 100vh;
}
.minh-vh-90 {
  min-height: 90vh;
}
::-moz-placeholder {
  color: #a3aacc;
  color: var(--silver);
  opacity: 1;
}
:-ms-input-placeholder {
  color: #a3aacc;
  color: var(--silver);
  opacity: 1;
}
::placeholder {
  color: #a3aacc;
  color: var(--silver);
  opacity: 1;
}
.scrollbar--dark {
  overflow-x: hidden;
  overflow-y: hidden;
  overflow: auto;
  scrollbar-color: #a3aacc #262d4d;
  scrollbar-color: var(--silver) var(--black);
  scrollbar-width: thin;
}
.scrollbar--dark::-webkit-scrollbar,
.scrollbar--dark::-webkit-scrollbar-track {
  background-color: #262d4d;
  background-color: var(--black);
}
.scrollbar--dark::-webkit-scrollbar {
  width: 6px;
  height: 6px;
}
.scrollbar--dark::-webkit-scrollbar-thumb {
  background-color: #a3aacc;
  background-color: var(--silver);
  border-radius: 9999px;
  border-radius: var(--border-radius-pill);
}
.scrollbar--light {
  scrollbar-color: #d6daed #fff;
  scrollbar-color: var(--smoke) var(--white);
  scrollbar-width: thin;
}
.scrollbar--light::-webkit-scrollbar,
.scrollbar--light::-webkit-scrollbar-track {
  background-color: #fff;
  background-color: var(--white);
}
.scrollbar--light::-webkit-scrollbar {
  width: 6px;
  height: 6px;
}
.scrollbar--light::-webkit-scrollbar-thumb {
  background-color: #d6daed;
  background-color: var(--smoke);
  border-radius: 9999px;
  border-radius: var(--border-radius-pill);
}
.sticky {
  position: -webkit-sticky;
  position: sticky;
}
@media screen and (min-width: 62.5em) {
  .bg-realtime-cta {
    background-image: url(assets/images/pusher-cta-aside.svg);
  }
}
.current-menu-item a {
  padding-bottom: 1px;
  border-bottom: 3px solid #db07eb;
  border-bottom: 3px solid var(--dragonfruit-light);
}
.hover-fill-white:hover svg path {
  fill: currentColor;
}
@media screen and (min-width: 60em) {
  .nav-item-spacing {
    margin-left: 1.75rem;
  }
}
@media screen and (min-width: 68.75rem) {
  .nav-item-spacing {
    margin-left: 2.5rem;
  }
}
@media screen and (min-width: 87.5rem) {
  .nav-item-spacing {
    margin-left: 3rem;
  }
}
@media screen and (min-width: 87.5em) {
  .ml2-xl {
    margin-left: 0.5rem;
    margin-left: var(--spacing-extra-small);
  }
  .mh2-xl,
  .mr2-xl {
    margin-right: 0.5rem;
    margin-right: var(--spacing-extra-small);
  }
  .mh2-xl {
    margin-left: 0.5rem;
    margin-left: var(--spacing-extra-small);
  }
  .pl2-xl {
    padding-left: 0.5rem;
    padding-left: var(--spacing-extra-small);
  }
}

.basis-40 {
  flex-basis: 40%;
}

.basis-60 {
  flex-basis: 60%;
}
@media screen and (min-width: 40em) {
  .basis-40-ns {
    flex-basis: 40%;
  }

  .basis-60-ns {
    flex-basis: 60%;
  }
}

.side-nav {
  box-shadow: rgba(207, 211, 230, 0.5) 0 2px 4px;
}

@media screen and (min-width: 62.5em) {
  .side-nav {
    box-shadow: rgba(207, 211, 230, 0.5) 2px 0 4px;
    width: 18rem;
  }
}

.tabbed-snippets pre {
  white-space: pre-wrap;
}

.tabbed-snippets ul {
  list-style-type: none;
  margin: 0 !important;
  padding: 0;
}

.tabbed-snippets pre:not(:first-of-type):not(.db) {
  display: none;
}

.tabbed-snippets button {
  border-bottom: 3px solid var(--transparent);
}
.tabbed-snippets button[aria-selected="true"] {
  border-bottom-color: var(--dragonfruit);
}

.scrollbar--light {
  scrollbar-color: var(--smoke-dark) var(--snow-light);
  scrollbar-width: thin;

  &::-webkit-scrollbar-track {
    background-color: var(--snow-light);
  }

  &::-webkit-scrollbar {
    width: 6px;
    height: 6px;
    background-color: var(--snow-light);
  }

  &::-webkit-scrollbar-thumb {
    background-color: var(--smoke-dark);
    border-radius: var(--border-radius-pill);
  }
}

table {
  border-spacing: 0;
  margin: var(--spacing-extra-large) 0;
}

th {
  background: var(--eggplant);
  color: var(--white);

  &:first-child {
    border-radius: var(--border-radius-2) 0 0;
  }
  &:last-child {
    border-radius: 0 var(--border-radius-2) 0 0;
  }
}

th,
td {
  padding: var(--spacing-small);
  border-bottom: 1px solid var(--smoke);
}

td {
  border-left: 1px solid var(--smoke);
  vertical-align: top;
}

td:last-child {
  border-right: 1px solid var(--smoke);
}

tr:last-child {
  td:first-child {
    border-bottom-left-radius: var(--border-radius-2);
  }
  td:last-child {
    border-bottom-right-radius: var(--border-radius-2);
  }
}

.nested-measure-wide p,
.nested-measure-wide ul,
.nested-measure-wide blockquote,
.nested-measure-wide h1,
.nested-measure-wide h2,
.nested-measure-wide h3,
.nested-measure-wide h4,
.nested-measure-wide h5,
.nested-measure-wide h6 {
  max-width: 34rem;
}

.nested-text-margin p,
.nested-text-margin ul,
.nested-text-margin blockquote,
.nested-text-margin h1,
.nested-text-margin h2,
.nested-text-margin h3,
.nested-text-margin h4,
.nested-text-margin h5,
.nested-text-margin h6 {
  margin-left: auto;
  margin-right: auto;
}

.nested-text-margin ul {
  padding: 0;
}

blockquote {
  background: var(--snow);
  border: 1px solid var(--smoke);
  border-radius: var(--border-radius-2);
  padding: var(--spacing-extra-extra-small) var(--spacing-medium);
}
