.dropdown-toggle-wrap:focus-within .dropdown,
.dropdown-toggle-wrap:focus .dropdown,
.dropdown-toggle-wrap:hover .dropdown {
  display: block;
}
.dropdown-toggle-wrap:focus-within .rotate-on-hover,
.dropdown-toggle-wrap:focus .rotate-on-hover,
.dropdown-toggle-wrap:hover .rotate-on-hover {
  transform: rotate(180deg);
}
.dropdown {
  width: 350px;
}
.dropdown,
.dropdown ul:before {
  transform: translateX(-50%);
  left: 50%;
}
.dropdown ul:before {
  position: absolute;
  top: -0.5rem;
  display: block;
  content: " ";
  width: 0;
  height: 0;
  border-left: 0.5rem solid transparent;
  border-right: 0.5rem solid transparent;
  border-bottom: 0.5rem solid #fff;
  border-bottom: 0.5rem solid var(--white);
}
.expand-flip {
  transition: transform 0.3s ease-in-out;
}
[aria-expanded="true"] > .expand-flip {
  transform: rotate(180deg);
}
